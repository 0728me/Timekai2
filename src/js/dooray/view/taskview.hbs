<div class="{{CSS_PREFIX}}task-left {{CSS_PREFIX}}left">
    <span>{{{taskTitle-tmpl}}}</span>
</div>
<div class="{{CSS_PREFIX}}task-right {{CSS_PREFIX}}right schedule-view clear">
{{#each events ~}}
<div class="{{CSS_PREFIX}}task-day" style="width:{{@root.width}}%;left:{{multiply @root.width @index}}%;min-height:{{@root.height}}px">
<div class="{{CSS_PREFIX}}task-day-wrap">
    <ul class="{{CSS_PREFIX}}task-list">
        {{#each morning.items ~}}
        <li data-id="{{stamp model}}"
        data-title="{{model.title}}"
        class="{{CSS_PREFIX}}task-item"
        style="background-color:{{model.bgColor}};color:{{model.color}}"><span>[출근 전]</span>{{{task-tmpl model}}}</li>
        {{/each ~}}
        {{#each lunch.items ~}}
        <li data-id="{{stamp model}}"
        data-title="{{model.title}}"
        class="{{CSS_PREFIX}}task-item"
        style="background-color:{{model.bgColor}};color:{{model.color}}"><span>[점심 전]</span>{{{task-tmpl model}}}</li>
        {{/each ~}}
        {{#each evening.items ~}}
        <li data-id="{{stamp model}}"
        data-title="{{model.title}}"
        class="{{CSS_PREFIX}}task-item"
        style="background-color:{{model.bgColor}};color:{{model.color}}"><span>[퇴근 전]</span>{{{task-tmpl model}}}</li>
        {{/each ~}}
    </ul>
</div>
</div>
{{/each ~}}
</div>
