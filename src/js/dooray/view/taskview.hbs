<div class="{{CSS_PREFIX}}task-left">
    <span>업무</span>
</div>
<div class="{{CSS_PREFIX}}task-right schedule-view clear">
{{#each events ~}}
<div class="{{CSS_PREFIX}}task-day" style="width:{{@root.width}}%;left:{{multiply @root.width @index}}%;min-height:{{@root.height}}px">
<div class="{{CSS_PREFIX}}task-day-wrap">
    {{! 출근전 }}
    {{#if morning.length ~}}
    <div class="{{CSS_PREFIX}}task-duedate-class" style="line-height:{{@root.lineHeight}}px"><strong>출근 전</strong></div>
    <ul class="{{CSS_PREFIX}}task-list">
        {{#each morning.items ~}}
        <li data-id="{{stamp model}}" 
        data-title="{{model.title}}" 
        class="{{CSS_PREFIX}}task-item" 
        style="line-height:{{@root.lineHeight}}px;color:{{model.color}}">{{{task-tmpl model}}}</li>
        {{/each ~}}
    </ul>
    {{/if ~}}

    {{! 점심전 }}
    {{#if lunch.length ~}}
    <div class="{{CSS_PREFIX}}task-duedate-class" style="line-height:{{@root.lineHeight}}px"><strong>점심 전</strong></div>
    <ul class="{{CSS_PREFIX}}task-list">
        {{#each lunch.items ~}}
        <li data-id="{{stamp model}}" 
        data-title="{{model.title}}" 
        class="{{CSS_PREFIX}}task-item" 
        style="line-height:{{@root.lineHeight}}px;color:{{model.color}}">{{{task-tmpl model}}}</li>
        {{/each ~}}
    </ul>
    {{/if ~}}

    {{! 퇴근전 }}
    {{#if evening.length ~}}
    <div class="{{CSS_PREFIX}}task-duedate-class" style="line-height:{{@root.lineHeight}}px"><strong>퇴근 전</strong></div>
    <ul class="{{CSS_PREFIX}}task-list">
        {{#each evening.items ~}}
        <li data-id="{{stamp model}}" 
        data-title="{{model.title}}" 
        class="{{CSS_PREFIX}}task-item" 
        style="line-height:{{@root.lineHeight}}px;color:{{model.color}}">{{{task-tmpl model}}}</li>
        {{/each ~}}
    </ul>
    {{/if ~}}
</div>
</div>
{{/each ~}}
</div>
