<div class="{{CSS_PREFIX}}month-dayname">
{{#each daynames}}
    <div class="{{CSS_PREFIX}}month-dayname-item"
         style="position: absolute;
                width: {{width}}%;
                left: {{left}}%">
        <span class="{{holiday day}}">{{label}}</span>
    </div>
{{/each}}
</div>
<div class="{{CSS_PREFIX}}month-weeks"></div>
